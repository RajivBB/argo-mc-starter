kind: Cluster
apiVersion: kind.x-k8s.io/v1alpha4
networking:
  podSubnet: "10.12.0.0/16"
  serviceSubnet: "10.13.0.0/16"
  disableDefaultCNI: true
  apiServerAddress: "10.0.2.52"
  apiServerPort: 9443
nodes:
- role: control-plane
  kubeadmConfigPatches:
  - |
    kind: ClusterConfiguration
    apiServer:
      certSANs:
      - "10.0.2.52"
      - "127.0.0.1"
- role: worker
